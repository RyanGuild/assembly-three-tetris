{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@assemblyscript/loader": "^0.9.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/redux-thunk": "^2.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.0",
    "react-three-fiber": "^4.0.17",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "three": "^0.113.2",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "npm run asbuild:untouched && npm run jsstart",
    "build": "npm run asbuild:optimized && npm run jsbuild",
    "jsstart": "react-scripts start",
    "jsbuild": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "asbuild:untouched": "asc assembly/index.ts -b public/as-api.wasm -t public/as-api.wat -d src/assembly-api/as-types.d.ts --validate --sourceMap --debug",
    "asbuild:optimized": "asc assembly/index.ts -b public/as-api.wasm -t public/as-api.wat -d src/assembly-api/as-types.d.ts --validate --sourceMap --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "assemblyscript": "github:AssemblyScript/assemblyscript"
  }
}
